  "bin": {
    "weather": "cli.js"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "dependencies": {
    "commander": "^3.0.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "yaml-lint": "^1.2.4"
  },
  "scripts": {
    "test": "node test.js",
    "start": "node cli.js --help",
    "version-production": "npm view ./package.json version",
    "version-fix": "npm version patch",
